public class SingletonCopy {
    private static volatile SingletonCopy instance;
    
    private SingletonCopy() {

    }

    public static SingletonCopy getInstance() {
        if (instance == null){
            synchronized (SingletonCopy.class) {
                if (instance == null) {
                    instance = new SingletonCopy();
                }
            }
        }
        return instance;
    }
}